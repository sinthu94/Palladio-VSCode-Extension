{
    "scopeName": "source.palladio",
    "fileTypes": [ "tpcm" ],
    "foldingStartMarker": "\\{\\s*$", 
    "foldingStopMarker": "^\\s*\\}", 
    "name": "Palladio",
    "patterns": [
      {
        "include": "#entities"
      },
      {
        "include": "#keywords"
      }, 
      {
        "include": "#storages"
      },
      {
        "include": "#punctuations"
      },
      {
        "include": "#supports"
      }
    ],

    "repository": {
      "entities": {
        "patterns": [
          
        ]
      },
      "keywords": {
        "patterns": [
          {
            "name": "keyword.declaration.palladio",
            "match": "component|datatype|failure|interface|assembly|connector|internal|provides|requires|seff" 
          },
          {
            "name": "keyword.control.import",
            "match": "import" 
          }
        ]
      },
      "storages": {
        "patterns": [
          {
            "name": "storage.type.class.palladio",
            "match": "allocation|repository|resourceenvironment|resourcetypes|system" 
          }
        ]
      },
      "punctuations": {
        "patterns": [
          {
            "name": "punctuation.accessor.palladio",
            "match": "->" 
          }
        ]
      },
      "supports": {
        "patterns": [
          {
            "name": "support.class.palladio",
            "match": "String|STRING|BOOL|BYTE" 
          }
        ]
      }
    }
  }
  
  
  
  
  